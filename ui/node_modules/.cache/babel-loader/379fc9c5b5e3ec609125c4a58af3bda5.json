{"ast":null,"code":"import axios from 'axios';\nimport store from '@/store/index.js';\nexport default {\n  name: 'UsersTable',\n  props: {\n    msg: String\n  },\n  computed: {\n    name() {\n      return store.state.name;\n    }\n\n  },\n\n  data() {\n    return {\n      users: [],\n      id: '',\n      name: '',\n      email: '',\n      privilege: ''\n    };\n  },\n\n  mounted() {\n    this.created();\n  },\n\n  methods: {\n    created() {\n      axios.get('http://127.0.0.1:8000/users/').then(response => {\n        this.users = response.data;\n      });\n    },\n\n    changeprivilege(user, changenum) {\n      axios.put('http://127.0.0.1:8000/users/' + user.id + '/', {\n        privilege: changenum\n      }, {\n        auth: {\n          username: 'paxx',\n          password: 'KSETpenisica43'\n        }\n      }).then(() => {\n        this.created();\n      });\n    },\n\n    deleteUser(user) {\n      axios.delete('http://127.0.0.1:8000/users/' + user.id + '/', {\n        auth: {\n          username: 'paxx',\n          password: 'KSETpenisica43'\n        }\n      }).then(() => {\n        this.created();\n      });\n    },\n\n    searchUser() {\n      axios.get('http://127.0.0.1:8000/users/?search=' + this.search).then(response => {\n        this.users = response.data;\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuCA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,KAAP,MAAkB,kBAAlB;AACA,eAAe;EACbC,IAAI,EAAE,YADO;EAEbC,KAAK,EAAE;IACLC,GAAG,EAAEC;EADA,CAFM;EAKbC,QAAQ,EAAE;IACRJ,IAAI,GAAE;MACJ,OAAOD,KAAK,CAACM,KAAN,CAAYL,IAAnB;IACF;;EAHQ,CALG;;EAUbM,IAAI,GAAE;IACJ,OAAM;MACJC,KAAK,EAAE,EADH;MAEJC,EAAE,EAAC,EAFC;MAGJR,IAAI,EAAC,EAHD;MAIJS,KAAK,EAAC,EAJF;MAKJC,SAAS,EAAC;IALN,CAAN;EAQD,CAnBY;;EAoBbC,OAAO,GAAE;IACP,KAAKC,OAAL;EACD,CAtBY;;EAwBbC,OAAO,EAAC;IACND,OAAO,GAAG;MACVd,KAAK,CAACgB,GAAN,CAAU,8BAAV,EACCC,IADD,CACMC,QAAO,IAAK;QAChB,KAAKT,KAAL,GAAYS,QAAQ,CAACV,IAArB;MACD,CAHD;IAGG,CALG;;IAMNW,eAAe,CAACC,IAAD,EAAMC,SAAN,EAAgB;MAC7BrB,KAAK,CAACsB,GAAN,CAAU,iCAA+BF,IAAI