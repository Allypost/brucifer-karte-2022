{"ast":null,"code":"import axios from 'axios';\nimport Dropdown from '../Dropdown.vue';\nexport default {\n  name: 'GuestsTable',\n  props: {\n    msg: String\n  },\n\n  data() {\n    return {\n      guests: [],\n      id: '',\n      name: '',\n      surname: '',\n      jmbag: '',\n      phone: '',\n      tag: '',\n      bought: '',\n      entered: '',\n      deleted: ''\n    };\n  },\n\n  mounted() {\n    this.created();\n  },\n\n  methods: {\n    created() {\n      axios.get('http://127.0.0.1:8000/guests/?search=Brucoši&search_fields=tag').then(response => {\n        console.log(response.data);\n        this.guests = response.data;\n      });\n    },\n\n    changebought(guest, changenum) {\n      axios.put('http://127.0.0.1:8000/guests/' + guest.id + '/', {\n        bought: changenum\n      }, {\n        auth: {\n          username: 'paxx',\n          password: 'KSETpenisica43'\n        }\n      }).then(() => {\n        this.created();\n      });\n    },\n\n    searchGuest() {\n      axios.get('http://127.0.0.1:8000/guests/?search=Brucoši ' + this.search + \"&search_fields=tag&search_fields=name&search_fields=surname\").then(response => {\n        this.guests = response.data;\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8CA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,QAAP,MAAqB,iBAArB;AACA,eAAe;EACbC,IAAI,EAAE,aADO;EAGbC,KAAK,EAAE;IACLC,GAAG,EAAEC;EADA,CAHM;;EAMbC,IAAI,GAAE;IACJ,OAAM;MACJC,MAAM,EAAE,EADJ;MAEJC,EAAE,EAAC,EAFC;MAGJN,IAAI,EAAC,EAHD;MAIJO,OAAO,EAAC,EAJJ;MAKJC,KAAK,EAAC,EALF;MAMJC,KAAK,EAAC,EANF;MAOJC,GAAG,EAAC,EAPA;MAQJC,MAAM,EAAC,EARH;MASJC,OAAO,EAAC,EATJ;MAUJC,OAAO,EAAC;IAVJ,CAAN;EAYD,CAnBY;;EAoBbC,OAAO,GAAE;IACP,KAAKC,OAAL;EACD,CAtBY;;EAuBbC,OAAO,EAAC;IACND,OAAO,GAAG;MACVjB,KAAK,CAACmB,GAAN,CAAU,gEAAV,EAECC,IAFD,CAEMC,QAAO,IAAK;QAChBC,OAAO,CAACC,GAAR,CAAYF,QAAQ,CAACf,IAArB;QACA,KAAKC,MAAL,GAAac,QAAQ,CAACf,IAAtB;MACD,CALD;IAMC,CARK;;IASNkB,YAAY,CAACC,KAAD,EAAOC,SAAP,EAAiB;MAC3B1B,KAAK,CAAC2B,GAAN,CAAU,kCAAgCF,KAAK,CAACjB,EAAtC,GAAyC,GAAnD,EA