 msg: String\r\n  },\r\n  data(){\r\n    return{\r\n      guests: [],\r\n      name:'',\r\n      surname:'',\r\n      jmbag:'',\r\n      phone:'',\r\n      tag:'',\r\n      bought:'',\r\n      entered:'',\r\n      deleted:'',\r\n    }\r\n    \r\n  },\r\n  created() {\r\n    axios.get('http://127.0.0.1:8000/guests/',)\r\n    .then(response => {\r\n      this.guests =response.data;\r\n    })\r\n  },\r\n  methods:{\r\n    changeboughtto1(guest){\r\n      axios.put('http://127.0.0.1:8000/guests/'+guest.jmbag+'/',\r\n      {bought:'1'},\r\n      {auth:{username:'paxx',password:'KSETpenisica43'}}\r\n      )\r\n      .then(()=>{\r\n        this.created;\r\n      }\r\n\r\n      )\r\n      \r\n    },\r\n    changeboughtto0(guest){\r\n      axios.put('http://127.0.0.1:8000/guests/'+guest.jmbag+'/',\r\n      {bought:'0'},\r\n      {auth:{username:'paxx',password:'KSETpenisica43'}}\r\n      )\r\n      .then(()=>{\r\n        this.created;\r\n      })\r\n      \r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n<style>\r\n#guests {\r\n  font-family: Arial, Helvetica, sans-serif;\r\n  border-collapse: collapse;\r\n  width: 100%;\r\n  text-align: center;\r\n}\r\n\r\n#guests td, #guests th {\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n  text-align: center;\r\n}\r\n\r\n#guests tr:nth-child(even){background-color: #f2f2f2;}\r\n\r\n#guests tr:hover {background-color: #ddd;}\r\n\r\n#guests th {\r\n  padding-top: 12px;\r\n  padding-bottom: 12px;\r\n  text-align: left;\r\n  background-color: rgba(0, 119, 255, 0.979);\r\n  color: white;\r\n  text-align: center;\r\n}\r\n#gumbary{\r\n  margin: 2px;\r\n  width: 40px;\r\n  height: 40px;\r\n  color: red;\r\n}\r\n#gumbarn{\r\n  margin: 2px;\r\n  width: 40px;\r\n  height: 40px;\r\n  color: green;\r\n}\r\n</style>\r\n\r\n\r\n"]},"metadata":{},"sourceType":"module"}