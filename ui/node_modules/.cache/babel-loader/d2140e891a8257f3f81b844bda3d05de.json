\" @click=\"changebought(guest,'0')\"> âœ“</button>\r\n                  </td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n        \r\n  \r\n      \r\n  </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n\r\n  name: 'GuestTag',\r\n  \r\n  props: {\r\n    msg: String\r\n  },\r\n  data() {\r\n   return {\r\n      guests: [],\r\n      slug:';'\r\n   }\r\n },\r\n\r\n\r\n\r\n  mounted() {\r\n    this.slug=this.$route.params.slug\r\n    this.created();\r\n     \r\n  },\r\n  methods:{\r\n    created() {\r\n    axios.get('http://127.0.0.1:8000/guests/?search='+this.slug+'&search_fields=tag',)\r\n    \r\n    .then(response => {\r\n      console.log(response.data)\r\n      this.guests =response.data;\r\n    })\r\n    },\r\n    changebought(guest,changenum){\r\n      axios.put('http://127.0.0.1:8000/guests/'+guest.id+'/',\r\n      {bought:changenum},\r\n      {auth:{username:'paxx',password:'KSETpenisica43'}}\r\n      )\r\n      .then(()=> {\r\n        this.created();\r\n      })\r\n      \r\n    },\r\n    searchGuest(){\r\n      axios.get('http://127.0.0.1:8000/guests/?search='+this.slug+' '+this.search+\"&search_fields=tag&search_fields=name&search_fields=surname\",)\r\n    .then(response => {\r\n      this.guests =response.data;\r\n    })\r\n    }\r\n  }\r\n}\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}