{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'UsersTable',\n  props: {\n    msg: String\n  },\n\n  data() {\n    return {\n      users: [],\n      id: '',\n      name: '',\n      email: '',\n      privilege: ''\n    };\n  },\n\n  mounted() {\n    this.created();\n  },\n\n  methods: {\n    created() {\n      axios.get('http://127.0.0.1:8000/lineup/').then(response => {\n        this.users = response.data;\n      });\n    },\n\n    changeprivilege(user, changenum) {\n      axios.put('http://127.0.0.1:8000/users/' + user.id + '/', {\n        privilege: changenum\n      }, {\n        auth: {\n          username: 'paxx',\n          password: 'KSETpenisica43'\n        }\n      }).then(() => {\n        this.created();\n      });\n    },\n\n    deleteUser(user) {\n      axios.delete('http://127.0.0.1:8000/users/' + user.id + '/', {\n        auth: {\n          username: 'paxx',\n          password: 'KSETpenisica43'\n        }\n      }).then(() => {\n        this.created();\n      });\n    },\n\n    searchUser() {\n      axios.get('http://127.0.0.1:8000/users/?search=' + this.search).then(response => {\n        this.users = response.data;\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAcA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,EAAE,YADO;EAEbC,KAAK,EAAE;IACLC,GAAG,EAAEC;EADA,CAFM;;EAKbC,IAAI,GAAE;IACJ,OAAM;MACJC,KAAK,EAAE,EADH;MAEJC,EAAE,EAAC,EAFC;MAGJN,IAAI,EAAC,EAHD;MAIJO,KAAK,EAAC,EAJF;MAKJC,SAAS,EAAC;IALN,CAAN;EAQD,CAdY;;EAebC,OAAO,GAAE;IACP,KAAKC,OAAL;EACD,CAjBY;;EAmBbC,OAAO,EAAC;IACND,OAAO,GAAG;MACVX,KAAK,CAACa,GAAN,CAAU,+BAAV,EACCC,IADD,CACMC,QAAO,IAAK;QAChB,KAAKT,KAAL,GAAYS,QAAQ,CAACV,IAArB;MAGD,CALD;IAKG,CAPG;;IAQNW,eAAe,CAACC,IAAD,EAAMC,SAAN,EAAgB;MAC7BlB,KAAK,CAACmB,GAAN,CAAU,iCAA+BF,IAAI,CAACV,EAApC,GAAuC,GAAjD,EACA;QAACE,SAAS,EAACS;MAAX,CADA,EAEA;QAACE,IAAI,EAAC;UAACC,QAAQ,EAAC,MAAV;UAAiBC,QAAQ,EAAC;QAA1B;MAAN,CAFA,EAICR,IAJD,CAIM,MAAI;QACR,KAAKH,OAAL;MACD,CAND;IAOD,CAhBK;;IAiBNY,UAAU,CAACN,IAAD,EAAM;MACdjB,KAAK,CAA